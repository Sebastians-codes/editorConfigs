	"editor.tokenColorCustomizations": {
		"[Vague]": {
			"textMateRules": [
				{
					"scope": [
						"variable.property",
						"variable.other.property",
						"meta.object-literal.key"
					],
					"settings": {
						"foreground": "#b6c4dc"
					}
				},
				{
					"scope": [
						"constant.numeric"
					],
					"settings": {
						"foreground": "#C2966B"
					}
				},
				{
					"scope": [
						"variable.parameter"
					],
					"settings": {
						"foreground": "#b8a2ba"
					}
				},
				{
					"scope": [
						"entity.name.type.class"
					],
					"settings": {
						"foreground": "#b06767"
					}
				},
				{
					"scope": [
						"entity.name.namespace"
					],
					"settings": {
						"foreground": "#748fa7c2"
					}
				},
				{
					"scope": [
						"keyword"
					],
					"settings": {
						"foreground": "#7894AB"
					}
				},
				{
					"scope": [
						"keyword.control"
					],
					"settings": {
						"foreground": "#748fa7c2"
					}
				},
				{
					"scope": [
						"entity.name.type.interface"
					],
					"settings": {
						"foreground": "#d57d7d"
					}
				},
				{
					"scope": [
						"keyword.operator"
					],
					"settings": {
						"foreground": "#CDCDCD"
					}
				}
			]
		}
	},
	"editor.semanticHighlighting.enabled": true,
	"editor.semanticTokenColorCustomizations": {
		"[Vague]": {
			"enabled": true,
			"rules": {
				"property": "#b6c4dc",
				"number": "#C2966B",
				"numeric": "#C2966B",
				"parameter": "#b8a2ba",
				"class": "#b06767",
				"namespace": "#748fa7c2",
				"keyword": "#7894AB",
				"controlKeyword": "#748fa7c2",
				"interface": "#d57d7d",
				"operator": "#CDCDCD"
			}
		}
	},
